{

  "manifest_version": 2,
  "name": "Imgblazeify",
  "version": "1.0",

  "description": "Redirect directly to imgblaze image.",
  "homepage_url": "https://github.com/jhanssen/imgblazeify",
  "icons": {
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png"
  },

  "permissions": [
    "activeTab"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/icon32.png",
    "default_title": "Imgblazeify"
  }
}
